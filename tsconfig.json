{
  "compilerOptions": {
    "baseUrl": "./src",
    "allowJs": true,
    "esModuleInterop": true,
    "jsx": "react-native",
    "lib": [
      "DOM",
      "ESNext"
    ],
    "moduleResolution": "node",
    "noEmit": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "target": "ESNext",
    "strict": true
  },
  "include": ["src"],
  "extends": "expo/tsconfig.base"
}